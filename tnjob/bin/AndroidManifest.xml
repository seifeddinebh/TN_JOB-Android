<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tnjob"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tnjob.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tnjob.Acceuil"
            android:label="@string/title_activity_acceuil" >
        </activity>
        <activity
            android:name="com.example.tnjob.Detaille"
            android:label="@string/title_activity_detaille" >
        </activity>
        <activity
            android:name="com.example.tnjob.Listoffre"
            android:label="@string/title_activity_listoffre" >
        </activity>
        <activity
            android:name="com.example.tnjob.Listoffrecat"
            android:label="@string/title_activity_listoffrecat" >
        </activity>
        <activity
            android:name="com.example.tnjob.Inscription"
            android:label="@string/title_activity_inscription" >
        </activity>
        <activity
            android:name="com.example.tnjob.Authentification"
            android:label="@string/title_activity_authentification" >
        </activity>
        <activity
            android:name="com.example.tnjob.EspaceEntre"
            android:label="@string/title_activity_espace_entre" >
        </activity>
    </application>

</manifest>
